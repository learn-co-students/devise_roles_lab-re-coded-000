<h1>Edit User</h1>
<%= form_for(@user) do |f| %>
<%= f.label :name %>
<%= f.text_field :name, class: 'Email' %>

<%= f.label :email %>
<%= f.email_field :email, class: 'Email' %>

<%= f.label :password %>
<%= f.password_field :password, class: 'Email' %>



<%= f.submit "Save changes" %>
<% end %>

